{
  "name": "chimp-project",
  "version": "0.0.0",
  "description": "Chimp",
  "repository": "http://github.com/xolvio/chimp",
  "author": "",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -rf dist && rm -rf chimp/node_modules && rm -rf chimp/package-lock.json && rm -rf test-project/node_modules && rm -rf test-project/package-lock.json",
    "watch": "npm-watch transpile",
    "transpile": "node ./scripts/transpile.js",
    "lint": "eslint --cache .",
    "lintfix": "eslint --cache --fix .",
    "test": "node ./scripts/test.js",
    "publish": "npm run transpile",
    "setup": "npm install --quite && node ./scripts/setup.js",
    "reset": "npm run clean && npm run setup"
  },
  "watch": {
    "transpile": {
      "patterns": "chimp/**/*.js",
      "ignore": [
        "node_modules"
      ],
      "quiet": true
    }
  },
  "distDir": "dist",
  "chimpDir": "chimp",
  "testProjectDir": "test-project",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "npm-watch": "^0.3.0",
    "shelljs": "^0.7.8",
    "source-map-support": "^0.5.0"
  },
  "dependencies": {}
}
