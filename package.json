{
  "name": "chimp",
  "version": "0.10.0",
  "description": "BDD acceptance testing package combining CucumberJS, PhantomJS, WebdriverIO and Chai. Plays nicely with Simian and SauceLabs",
  "keywords": [
    "simian",
    "meteor",
    "velocity",
    "bdd",
    "atdd",
    "cucumber",
    "webdriverio",
    "selenium",
    "phantom",
    "testing",
    "saucelabs"
  ],
  "author": "Sam Hatoum <sam@xolv.io> (http://xolv.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xolvio/chimp"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/xolvio/chimp/issues"
  },
  "scripts": {
    "test": "jest -i && ./bin/chimp --tags=@cli --browser=phantomjs --debug && ./bin/chimp --tags=~@cli --debug && ./bin/chimp --browser=firefox --tags=~@cli --debug && ./bin/chimp --browser=phantomjs --tags=~@cli --debug"
  },
  "main": "lib/chimp.js",
  "bin": {
    "chimp": "./bin/chimp"
  },
  "dependencies": {
    "async": "~0.9.0",
    "bluebird": "^2.9.33",
    "chai": "~3.0.0",
    "chai-as-promised": "~5.1.0",
    "cucumber": "^0.5.2",
    "ddp": "xolvio/node-ddp-client#master",
    "freeport": "~1.0.5",
    "minimist": "~1.1.1",
    "phantomjs-bin": "~1.0.1",
    "progress": "^1.1.8",
    "saucelabs": "~0.1.1",
    "selenium-standalone": "^4.5.3",
    "webdriverio": "~3.0.5",
    "chokidar": "~1.0.3",
    "underscore": "~1.8.3",
    "loglevel": "~1.3.1",
    "fs-extra": "~0.22.1",
    "request": "2.58.0",
    "hapi": "8.8.0",
    "chimp-widgets": "0.2.0"
  },
  "devDependencies": {
    "jest-cli": "0.4.15"
  }
}
